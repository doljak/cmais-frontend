# all files with .something are skipped
#RewriteCond %{REQUEST_URI} \..+$
#RewriteCond %{REQUEST_URI} !\.html$
#RewriteRule .* - [L]
# the others are redirected to the front web controller
#RewriteRule ^(.*)$ index.php [QSA,L]

